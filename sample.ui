<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="relief">raised</property>
    <property name="takefocus">true</property>
    <property name="width">300</property>
    <child>
      <object class="tk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="width">300</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
        <child>
          <object class="tk.Button" id="buttonCapture">
            <property name="anchor">n</property>
            <property name="command" type="command" cbtype="simple">startScanning</property>
            <property name="compound">bottom</property>
            <property name="default">normal</property>
            <property name="justify">center</property>
            <property name="relief">raised</property>
            <property name="text" translatable="yes">Capture</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.18</property>
              <property name="rely">0.14</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="buttonIntegration">
            <property name="command" type="command" cbtype="simple">integrateScene</property>
            <property name="default">normal</property>
            <property name="justify">left</property>
            <property name="relief">raised</property>
            <property name="text" translatable="yes">Integration</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.18</property>
              <property name="rely">0.34</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Entry" id="filepathEntry">
            <property name="exportselection">true</property>
            <property name="takefocus">true</property>
            <property name="text" translatable="yes">File Path</property>
            <property name="validate">none</property>
            <layout manager="place">
              <property name="anchor">nw</property>
              <property name="relx">0.49</property>
              <property name="rely">0.8</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="buttonBrowse">
            <property name="anchor">n</property>
            <property name="command" type="command" cbtype="simple">displayPLY</property>
            <property name="compound">left</property>
            <property name="font">TkDefaultFont</property>
            <property name="justify">right</property>
            <property name="text" translatable="yes">Browse</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="relx">0.19</property>
              <property name="rely">0.77</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="pipeline">
            <property name="state">normal</property>
            <property name="text" translatable="yes">Capture pipelines</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="relx">0.26</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="preview">
            <property name="justify">left</property>
            <property name="relief">flat</property>
            <property name="takefocus">false</property>
            <property name="text" translatable="yes">Preview PLY
</property>
            <layout manager="place">
              <property name="anchor">n</property>
              <property name="relx">0.18</property>
              <property name="rely">0.60</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Button" id="buttonDelete">
            <property name="command" type="command" cbtype="simple">deleteLastScan</property>
            <property name="justify">left</property>
            <property name="text" translatable="yes">Delete Instace</property>
            <layout manager="place">
              <property name="relx">0.68</property>
              <property name="rely">0.14</property>
              <property name="x">0</property>
              <property name="y">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
